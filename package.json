{
    "name": "flutter-androidx-checker",
    "displayName": "flutter-androidx-checker",
    "description": "flutter androidx checker",
    "version": "1.0.2",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "flutter",
        "androidx"
    ],
    "publisher": "erlangparasu",
    "homepage": "https://github.com/erlangp/flutter-androidx-checker/blob/master/README.md",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/erlangp/flutter-androidx-checker"
    },
    "activationEvents": [
        "onLanguage:yaml"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.checkDependencies",
                "title": "Check AndroidX dependencies (Flutter)"
            },
            {
                "command": "extension.openVirDocResult",
                "title": "Open Flutter AndroidX Checker Result"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.checkDependencies",
                    "when": "editorTextFocus && resourceExtname == .yaml && resourceFilename =~ /pubspec.yaml/",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.14.6",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28"
    },
    "dependencies": {
        "@types/glob": "^7.1.1",
        "@types/js-yaml": "^3.12.1",
        "@types/table": "^4.0.5",
        "glob": "^7.1.3",
        "js-yaml": "^3.13.1",
        "table": "^5.2.3"
    }
}
